n=int(input())
i=0
while i<n:
    if i%2==0:
        print("I hate",end=" ")
        if(i==n-1):
            print("it")
        else:
            print("that",end=" ")
    else:
        print("I love ",end=" ")
        if(i==n-1):
            print("it")
        else:
            print("that",end=" ")
    i=i+1
